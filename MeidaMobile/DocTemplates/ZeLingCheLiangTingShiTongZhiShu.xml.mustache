<?xml version="1.0" encoding="UTF-8"?>
<pdf:template>
	{{! 责令车辆停驶通知书 }}
	<pdf:paper width="595.276" height="841.89" fontName="SimSun" fontSize="13" />
	<pdf:content>
		<pdf:table rect="{13,8.5},{188,128.5}" borderWidth="0.5">
			<pdf:tr height="128.5">
				<pdf:td width="188">
					<pdf:text point="57.5,9.5" fontName="SimHei" fontSize="21.5">责令车辆停驶通知书</pdf:text>
					<pdf:line fromPoint="6,27" toPoint="59,27" lineWidth="0.5" />
					<pdf:text point="25,22" static="NO" maxWidth="45" >{{ citizenName }}</pdf:text>
					<pdf:text point="61,22.5">：</pdf:text>
					<pdf:text point="10,34">你于</pdf:text>
					<pdf:line fromPoint="20,38.8" toPoint="36,38.8" lineWidth="0.5" />
					<pdf:text point="26,34" static="NO">{{ happenDate.year }}</pdf:text>
					<pdf:text point="36.5,34">年</pdf:text>
					<pdf:line fromPoint="43,38.8" toPoint="60,38.8" lineWidth="0.5" />
					<pdf:text point="52,34" maxWidth="" static="NO">{{ happenDate.month }}</pdf:text>
					<pdf:text point="61,34">月</pdf:text>
					<pdf:line fromPoint="67.5,38.8" toPoint="84,38.8" lineWidth="0.5" />
					<pdf:text point="75,34" static="NO">{{ happenDate.day }}</pdf:text>
					<pdf:text point="85,34">日 驾驶牌号为</pdf:text>
					<pdf:line fromPoint="120,38.8" toPoint="166,38.8" lineWidth="0.5" />
					<pdf:text point="127,34" maxWidth="" static="NO">{{ automobileNumber }}</pdf:text>
					<pdf:text point="168,34">的车辆，</pdf:text>
					<pdf:text point="2,46">在</pdf:text>
					<pdf:line fromPoint="9,51" toPoint="46,51" lineWidth="0.5" />
					<pdf:text point="12,46" maxWidth="37" static="NO">{{ placePrefix }}</pdf:text>
					<pdf:text point="48.5,46">公路K</pdf:text>
					<pdf:line fromPoint="64,51" toPoint="110,51" lineWidth="0.5" />
					<pdf:text point="67,46" maxWidth="43" static="NO">{{ stationStart }}</pdf:text>
					<pdf:text point="111,46">因</pdf:text>
					<pdf:line fromPoint="118,51" toPoint="174,51" lineWidth="0.5" />
					<pdf:text point="120,46" maxWidth="55" static="NO">{{ caseDescription }}</pdf:text>
					<pdf:text point="177,46">根据</pdf:text>
					<pdf:text point="1,60">①《中华人民共和国公路法》第</pdf:text>
					<pdf:line fromPoint="69,64" toPoint="86,64" lineWidth="0.5" />
					<pdf:text point="71.5,60" static="YES">八十五</pdf:text>
					<pdf:text point="88,60">条第</pdf:text>
					<pdf:line fromPoint="99,64" toPoint="115,64" lineWidth="0.5" />
					<pdf:text point="104,60" static="YES">二</pdf:text>
					<pdf:text point="115.5,60">款、②《路政管理规定》第</pdf:text>
					<pdf:line fromPoint="170,64" toPoint="183,64" lineWidth="0.5" />
					<pdf:text point="170,60" maxWidth="13" static="YES">三十八</pdf:text>
					<pdf:text point="183,60">条</pdf:text>
					<pdf:text point="1,72">之规定，现责令你将该车辆临时停放于</pdf:text>
					<pdf:line fromPoint="75,78" toPoint="170,78" lineWidth="0.5" />
					<pdf:text point="94.5,72.5" maxWidth="90" static="NO">{{ parkingAddress }}</pdf:text>
					<pdf:text point="186,60">，</pdf:text>
					<pdf:text point="1,83">并在收到本通知书之日起</pdf:text>
					<pdf:line fromPoint="55,88" toPoint="70,88" lineWidth="0.5" />
					<pdf:text point="68,84" maxWidth="12" static="NO">{{ periodLimit }}</pdf:text>
					<pdf:text point="72,83">日内到</pdf:text>
					<pdf:line fromPoint="95,88" toPoint="162,88" lineWidth="0.5" />
					<pdf:text point="96,84" maxWidth="65" static="NO">{{ officeAddress }}</pdf:text>
					<pdf:text point="163,83">接受处理。</pdf:text>
					<pdf:text point="27,103">特此通知</pdf:text>
					<pdf:text point="135,90">（执法机关印章）</pdf:text>
					<pdf:text point="121,114">   年    月    日</pdf:text>
                    <pdf:text point="110,114" static="NO">{{sendDate.year}}</pdf:text>
                    <pdf:text point="128,114" static="NO">{{sendDate.month}}</pdf:text>
                    <pdf:text point="140,114" static="NO">{{sendDate.day}}</pdf:text>
					<pdf:text point="7.5,118" fontSize="10.5">（第一联：交当事人）</pdf:text>
				</pdf:td>
			</pdf:tr>
		</pdf:table>
	</pdf:content>
</pdf:template>